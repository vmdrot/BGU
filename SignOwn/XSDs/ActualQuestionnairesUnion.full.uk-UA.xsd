            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="IsSecondaryMarketPurchase" type="xs:boolean" type_ukr="логічний тип(так/ні)" displayName="придбання акцій (паїв) банку на вторинному ринку" description="..." field_order="2">
            <xs:annotation>
