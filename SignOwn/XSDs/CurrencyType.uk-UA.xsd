<?xml version="1.0" encoding="utf-8"?>
<xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="CurrencyType" type="CurrencyType" />
  <xs:simpleType name="CurrencyType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="None" description="Не вказано" />
      <xs:enumeration value="NationalCurrency" description="Національна валюта" />
      <xs:enumeration value="ForeignCurrencyAndBankingMetals" description="Іноземна валюта й банківські метали" />
    </xs:restriction>
  </xs:simpleType>
</xs:schema>