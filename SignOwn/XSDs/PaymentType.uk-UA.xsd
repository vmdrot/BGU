<?xml version="1.0" encoding="utf-8"?>
<xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="PaymentType" type="PaymentType" />
  <xs:simpleType name="PaymentType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="None" description="Не вказано" enum_value="0" />
      <xs:enumeration value="Cash" description="Готівка" enum_value="1" />
      <xs:enumeration value="WireTransfer" description="Безготівка" enum_value="2" />
      <xs:enumeration value="Other" description="Інше" enum_value="3" />
    </xs:restriction>
  </xs:simpleType>
</xs:schema>